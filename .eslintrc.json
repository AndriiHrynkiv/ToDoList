{
    "extends": ["@eleks/eleks", "angular"],
    "env": {
       "es6": false
     },
     "parserOptions": {
       "ecmaVersion": 5,
       "sourceType": "script"
     },
     "rules": {
       "space-before-function-paren": [
         1,
         "never"
       ],
       "curly": [
         1,
         "multi"
       ],
       "one-var": 1,
       "object-shorthand": 1
     }
 }